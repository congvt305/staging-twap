<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="footer">
            <block class="Magento\Framework\View\Element\Html\Links" name="footer.store.links" after="-">
                <arguments>
                    <argument name="css_class" xsi:type="string">store links</argument>
                </arguments>
                <block class="Magento\Framework\View\Element\Html\Link" name="footer-store-locator-link" after="-">
                    <arguments>
                        <argument name="class" xsi:type="string" translate="true">location</argument>
                        <argument name="path" xsi:type="string">stores/info/list</argument>
                        <argument name="label" xsi:type="string" translate="true">Store Locator</argument>
                        <argument name="sortOrder" xsi:type="number">120</argument>
                    </arguments>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
