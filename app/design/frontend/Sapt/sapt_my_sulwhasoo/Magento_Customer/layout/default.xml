<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="register-link">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Sign up</argument>
            </arguments>
        </referenceBlock>
        <referenceBlock name="authorization-link-login">
            <arguments>
                <argument name="label" xsi:type="string" translate="true">Login</argument>
            </arguments>
        </referenceBlock>
        <move element="register-link" destination="header.links" after="authorization-link-login"/>
    </body>
</page>
