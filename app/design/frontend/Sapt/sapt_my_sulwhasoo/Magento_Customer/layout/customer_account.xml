<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="sidebar.main">
            <block class="Magento\Framework\View\Element\Template" name="sidebar.main.account_nav.order" template="Magento_Theme::html/collapsible.phtml" before="-">
                <arguments>
                    <argument name="block_css" xsi:type="string">account-nav</argument>
                    <argument name="block_id" xsi:type="string">account-order</argument>
                    <argument name="block_title" xsi:type="string">My Order</argument>
                </arguments>
                <block class="Magento\Customer\Block\Account\Navigation" name="customer_account_navigation_order" before="-">
                    <arguments>
                        <argument name="css_class" xsi:type="string">nav items</argument>
                    </arguments>
                    <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-order-history">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Order/Refund/Exchange</argument>
                            <argument name="path" xsi:type="string">sales/order/history</argument>
                            <argument name="sortOrder" xsi:type="number">180</argument>
                        </arguments>
                    </block>
                    <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-order-review">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Review</argument>
                            <argument name="path" xsi:type="string">sales/order/history</argument>
                            <argument name="sortOrder" xsi:type="number">180</argument>
                        </arguments>
                    </block>
                </block>
            </block>

            <block class="Magento\Framework\View\Element\Template" name="sidebar.main.account_nav.profile" template="Magento_Theme::html/collapsible.phtml" after="sidebar.main.account_nav">
                <arguments>
                    <argument name="block_css" xsi:type="string">account-nav</argument>
                    <argument name="block_id" xsi:type="string">account-profile</argument>
                    <argument name="block_title" xsi:type="string">My Profile</argument>
                </arguments>
                <block class="Magento\Customer\Block\Account\Navigation" name="customer_account_navigation_profile" before="-">
                    <arguments>
                        <argument name="css_class" xsi:type="string">nav items</argument>
                    </arguments>
                    <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-profile-information">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Personal Information</argument>
                            <argument name="path" xsi:type="string">customer/account/edit</argument>
                            <argument name="sortOrder" xsi:type="number">180</argument>
                        </arguments>
                    </block>
                    <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-profile-address">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Address</argument>
                            <argument name="path" xsi:type="string">customer/address</argument>
                            <argument name="sortOrder" xsi:type="number">180</argument>
                        </arguments>
                    </block>
                </block>
            </block>

            <block class="Magento\Framework\View\Element\Template" name="sidebar.main.account_nav.membership" template="Magento_Theme::html/collapsible.phtml" after="sidebar.main.account_nav.profile">
                <arguments>
                    <argument name="block_css" xsi:type="string">account-nav</argument>
                    <argument name="block_id" xsi:type="string">account-membership</argument>
                    <argument name="block_title" xsi:type="string">My Membership</argument>
                </arguments>
                <block class="Magento\Customer\Block\Account\Navigation" name="customer_account_navigation_membership" before="-">
                    <arguments>
                        <argument name="css_class" xsi:type="string">nav items</argument>
                    </arguments>
                    <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-membership">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">Coupon/Point</argument>
                            <argument name="path" xsi:type="string">customercoupon/coupon/index</argument>
                            <argument name="sortOrder" xsi:type="number">180</argument>
                        </arguments>
                    </block>
                </block>
            </block>

            <block class="Magento\Framework\View\Element\Template" name="sidebar.main.account_nav.service" template="Magento_Theme::html/collapsible.phtml" after="sidebar.main.account_nav.membership">
                <arguments>
                    <argument name="block_css" xsi:type="string">account-nav</argument>
                    <argument name="block_id" xsi:type="string">account-service</argument>
                    <argument name="block_title" xsi:type="string">Service Center</argument>
                </arguments>
                <block class="Magento\Customer\Block\Account\Navigation" name="customer_account_navigation_service" before="-">
                    <arguments>
                        <argument name="css_class" xsi:type="string">nav items</argument>
                    </arguments>
                    <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-service">
                        <arguments>
                            <argument name="label" xsi:type="string" translate="true">1:1 Inquiry</argument>
                            <argument name="path" xsi:type="string">customercoupon/coupon/index</argument>
                            <argument name="sortOrder" xsi:type="number">180</argument>
                        </arguments>
                    </block>
                </block>
            </block>
        </referenceContainer>
        <referenceBlock name="sidebar.main.account_nav">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-navigation-account-history">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">My History</argument>
                    <argument name="path" xsi:type="string">customer/account/history</argument>
                    <argument name="sortOrder" xsi:type="number">180</argument>
                </arguments>
            </block>
        </referenceBlock>
        <referenceBlock name="customer-account-navigation-address-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-account-edit-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-delimiter-2" remove="true"/>
        <referenceBlock name="customer-account-navigation-delimiter-1" remove="true"/>
        <referenceBlock name="customer-account-navigation-account-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-return-history-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-pos-integration-points" remove="true"/>
        <referenceBlock name="customer-account-coupon-wallet" remove="true"/>
        <referenceBlock name="customer-account-navigation-ticket-link" remove="true"/>
        <referenceBlock name="company-customer-account-navigation-orders-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-return-history-link" remove="true"/>
        <referenceBlock name="customer-account-navigation-orders-link" remove="true"/>
    </body>
</page>
