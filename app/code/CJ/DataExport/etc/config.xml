            </cj_scheduleexport>
        </tw_lageige_website>
        <base>
            <cj_scheduleexport>
                <general>
                    <enable_data_marketing>1</enable_data_marketing>
                </general>
            </cj_scheduleexport>
        </base>
    </websites>
</config>
