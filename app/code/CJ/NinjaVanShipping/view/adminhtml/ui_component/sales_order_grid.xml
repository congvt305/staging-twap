<?xml version="1.0"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" component="Magento_Ui/js/grid/tree-massactions">
            <action name="print_ninjavan_waybill">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">print_ninjavan_waybill</item>
                        <item name="label" xsi:type="string" translate="true">Print NinjaVan Waybill</item>
                        <item name="url" xsi:type="url" path="ninjavan/order/massprintwaybill"/>
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Print NinjaVan Waybill</item>
                            <item name="message" xsi:type="string" translate="true">Are you sure you want to print NinjaVan Waybill for selected order(s)?</item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
