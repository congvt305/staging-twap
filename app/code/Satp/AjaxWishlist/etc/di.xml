<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\Wishlist\CustomerData\Wishlist" type="Satp\AjaxWishlist\CustomerData\Wishlist"/>
    <preference for="Satp\AjaxWishlist\Api\AjaxWishlistInterface" type="Satp\AjaxWishlist\Model\Api\AjaxWishlist"/>
    <type name="Magento\CatalogWidget\Block\Product\ProductsList">
        <plugin name="Satp_AjaxWistlist::widget_view_model" type="Satp\AjaxWishlist\Plugin\CatalogWidgetProductList"/>
    </type>

    <type name="Magento\Wishlist\Model\Wishlist">
        <plugin name="Satp_AjaxWistlist::enable_social_shared" type="Satp\AjaxWishlist\Plugin\Model\Wishlist"/>
    </type>
</config>
