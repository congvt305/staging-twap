<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="magento_rma" resource="default" engine="innodb" comment="RMA LIst">
        <column xsi:type="varchar" name="shipping_preference" nullable="true" length="255"
                comment="Shipping Carrier Preference"/>
        <column xsi:type="varchar" name="customer_custom_phone" nullable="true" length="255"
                comment="Customer Custom Phone"/>
        <column xsi:type="varchar" name="rma_customer_name" nullable="true" length="255"
                comment="RMA Customer Name"/>
    </table>
</schema>
