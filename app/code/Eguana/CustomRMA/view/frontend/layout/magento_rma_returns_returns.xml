<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="magento.rma.info.button">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">Eguana_CustomRMA::order/button.phtml</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="rma.returns.returns">
            <arguments>
                <argument name="custom_name_view_model" xsi:type="object">Eguana\CustomRMA\ViewModel\CustomName</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
