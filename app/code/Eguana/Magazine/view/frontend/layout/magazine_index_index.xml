<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Discover and Enjoy</title>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Eguana\Magazine\Block\ListBlock" name="magazine.list.top.content" template="Eguana_Magazine::top-content.phtml" before="-">
                <arguments>
                    <argument name="view_model" xsi:type="object">Eguana\Magazine\ViewModel\Magazine</argument>
                </arguments>
            </block>
            <block class="Eguana\Magazine\Block\ListBlock" name="magazine.list" template="Eguana_Magazine::list.phtml" />
            <block class="Eguana\Magazine\Block\ListBlock" name="magazine.list.bottom.content" template="Eguana_Magazine::bottom-content.phtml" after="-">
                <arguments>
                    <argument name="view_model" xsi:type="object">Eguana\Magazine\ViewModel\Magazine</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
